<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record model="ir.module.category" id="escuela_rol_category">
            <field name="name">Rol Escuela</field>
            <field name="description">Rol Escuela</field>
            <field name="sequence">10</field>
        </record>


        <record id="REscuela_base" model="res.groups">
            <field name="name">Alumno</field>
            <field name="category_id" ref="escuela_rol_category"/>
        </record>


        <record id="REscuela_responsable" model="res.groups">
            <field name="name">Responsable</field>
            <field name="implied_ids" eval="[(4, ref('REscuela_base'))]"/>
            <field name="category_id" ref="escuela_rol_category"/>
        </record>


    </data>
</odoo>