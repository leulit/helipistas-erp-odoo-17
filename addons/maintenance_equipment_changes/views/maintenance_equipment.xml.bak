<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>

        <record id="leulit_20230424_1218_form" model="ir.ui.view">
            <field name="name">leulit_20230424_1218_form</field>
            <field name="model">maintenance.equipment</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="maintenance.hr_equipment_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='maintenance']" position="after">
                    <page string="Histórico Pieza" name="historico_pieza_helipistas" attrs="{'invisible': [('is_historico_pieza', '=', False)]}">
                        <field name="is_historico_pieza" invisible="1"/>
                        <separator string="Histórico"/>
                        <field name="historico_pieza">
                            <tree editable="bottom" create="true" edit="false">
                                <field name="date"/>
                                <field name="old_production_lot_id"/>
                                <field name="new_production_lot_id"/>
                                <field name="tsn_inicio"/>
                                <field name="tsn_actual"/>
                                <field name="tso_inicio"/>
                                <field name="tso_actual"/>
                                <field name="ng_inicio"/>
                                <field name="ng_actual"/>
                                <field name="nf_inicio"/>
                                <field name="nf_actual"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>

    </data>
</odoo>